<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" id="main" stylesheets="@../css/style.css"
            xmlns="http://javafx.com/javafx/17.0.6" fx:controller="com.bj.controller.MainController">
    <top>
        <Button alignment="CENTER" contentDisplay="CENTER" onAction="#onStartButtonClick" prefHeight="78.0"
                prefWidth="298.0" text="Start Game" textAlignment="CENTER" BorderPane.alignment="CENTER">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <effect>
                <Glow/>
            </effect>
            <BorderPane.margin>
                <Insets top="33.0"/>
            </BorderPane.margin>
        </Button>
    </top>
</BorderPane>
